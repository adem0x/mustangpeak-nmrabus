// *****************************************************************************
//  procedure FlushResponseList
//     Parameters:
//     Returns:
//
//     Description:
//
// *****************************************************************************
procedure FlushResponses;
var
  i, j: Integer;
begin
  NMRABus.Responses.SimpleResponseFlags := 0;
  NMRABus.Responses.ListIndex := 0;
  NMRABus.Responses.SimpleIndexMask := 0;
  NMRABus.Responses.iState := 0;
  NMRABus.Responses.iCDI := 0;
  for i := 0 to LEN_QUEUED_RESPONSE_LIST - 1 do
  begin
    NMRABus.Responses.List[i].ResponseCode := 0;
    NMRABus.Responses.List[i].DestinationNodeAlias := 0;
    for j := 0 to CAN_DATA_LEN - 1 do
      NMRABus.Responses.List[i].DataBytes[j] := 0;
  end;
end;

// *****************************************************************************
//  procedure FlushLists
//     Parameters:
//     Returns:
//
//     Description: Clears the lists
//
// *****************************************************************************
procedure FlushLists;
begin
  CANStorage_FlushList(@TX_CANBufferList);                                      // Flush any pending packets
  CANStorage_FlushList(@TX_NMRAnetBufferList);
  FlushResponses;
end;

// *****************************************************************************
//  procedure TryTransmitCANLayerMsg
//     Parameters:
//     Returns:
//
//     Description: Clears the lists
//
// *****************************************************************************
function TryTransmitCANLayerMsg(VariableField: DWord; Buffer: PCANBuffer): Boolean;
begin
  Result := False;
  if not CANStorage_ListFull(@TX_CANBufferList) then
  begin
    NMRABusUtilities_CreateCANControlFramePacket(FT_RID, Buffer);
    if CANStorage_AddToList(@TX_CANBufferList, Buffer) then
    begin
      StartCANMessageEngine();
      Result := True
    end
  end
end;

// *****************************************************************************
// CAN Layer Message Handlers
// *****************************************************************************
procedure HandleEqualAliasID(CANBuffer: PCANBuffer; var LocalFlags: Word);
begin
  if (CANBuffer^.ID and FT_CID_MASK <= FT_CID0) and (CANBuffer^.ID and FT_CID_MASK >= FT_CID6) then // If it is a CID message then force calling node to try again for an Alias this one is ours
    LocalFlags := LocalFlags or SRF_SEND_RID
  else
    LocalFlags := LocalFlags or SRF_DUPLICATE_ALIAS
end;

procedure HandleAliasMappingDefinition(CANBuffer: PCANBuffer; var LocalFlags: Word);
var
  NodeID: TNodeID;
begin
  NMRABusUtilities_PacketBytesToNodeID(@CANBuffer^, NodeID);                    // Extract the 48 Bit ID
  if NMRABusUtilities_EqualNodeID(NodeID) then                                  // If addressed to us then respond
    LocalFlags := LocalFlags or SRF_SEND_PCER
end;

procedure HandleAliasMappingEnquiry(CANBuffer: PCANBuffer; var LocalFlags: Word);
var
  NodeID: TNodeID;
begin
  if CANBuffer^.DataCount > 0 then
  begin
    NMRABusUtilities_PacketBytesToNodeID(CANBuffer, NodeID);              // Extract the 48 Bit ID
    if NMRABusUtilities_EqualNodeID(NodeID) then                          // If addressed to us then respond
      LocalFlags := LocalFlags or SRF_SEND_AMD;
  end else
    LocalFlags := LocalFlags or SRF_SEND_AMD;
end;

// ****************************************************************************
// procedure HandlePacketReceivedOnCANLayer
//
//  Description:
//                 All CAN Layer messages are high priority and are dealt with immediately
//
// ****************************************************************************
procedure HandlePacketReceivedOnCANLayer(CANBuffer: PCANBuffer);
var
  DoDefault: Boolean;
  LocalFlags: Word;
begin
  DoDefault := True;
  if CANLayerReceiveHook <> nil then
    CANLayerReceiveHook(CANBuffer, DoDefault);
  if DoDefault then
  begin
    LocalFlags := 0;
    // This is ALWAYS checked regardless of permitted state
    if NMRABusUtilities_EqualAliasID(CANBuffer^.ID and MASK_SOURCE_ALIAS) then  
      HandleEqualAliasID(CANBuffer, LocalFlags);
    
    if NMRABus_Permitted then
    begin
      if CANBuffer^.ID and FT_UNADDRESSED_MASK = FT_AME then 
        HandleAliasMappingEnquiry(CANBuffer, LocalFlags)
      else
      if CANBuffer^.ID and FT_UNADDRESSED_MASK = FT_AMD then
         HandleAliasMappingDefinition(CANBuffer, LocalFlags)
    end;
    NMRABus.Responses.SimpleResponseFlags := NMRABus.Responses.SimpleResponseFlags or LocalFlags
  end
end;

// *****************************************************************************
// NMRAnet Layer Message Handlers
// *****************************************************************************
procedure HandleUnAddressedMessage(CANBuffer: PCANBuffer; var LocalFlags: Word);
begin
  if (CANBuffer^.ID and MTI_TYPE_MASK = FT_VERIFY_NODE_ID_NUMBER_NO_DEST) then
    LocalFlags := LocalFlags or SRF_SEND_VERIFIED_NODE;
end;

procedure HandleDatagramMessage(CANBuffer: PCANBuffer; var LocalFlags: Word);
begin
  if NMRABusUtilities_IsAddressedMessageToNode(CANBuffer) then
  begin
  end

end;

procedure HandleAddressedMessage(CANBuffer: PCANBuffer; var LocalFlags: Word);
var
  MTICode: Word;
begin
  if NMRABusUtilities_IsAddressedMessageToNode(CANBuffer) then
  begin
    MTICode := NMRABusUtilities_PackBytesLo(CANBuffer);
    if MTICode and $00FF = SFT_VERIFY_NODE_DEST then 
      LocalFlags := LocalFlags or SRF_SEND_VERIFIED_NODE
    else
    if MTICode = MTI_PROTOCOL_SUPPORT_INQUIRY   
      then LocalFlags := LocalFlags or SRF_SEND_PROTOCOL_SUPPORT_RESULT
    else
    if MTICode = MTI_PROTOCOL_SUPPORT_RESPONSE then
    begin
   //   NMRABus.ProtocolSupportInfo.AliasID := NMRABusUtilities_ExtractDestinationAlias(CANBuffer);
  //    for i := 0 to 5 do
  //      NMRABus.ProtocolSupportInfo.ProtocolFlags[i] := CANBuffer^.DataBytes[i + 2];
  //    LocalFlags := LocalFlags or RMF_SEND_PROTOCOL_SUPPORT_INQURIY_VALID    // Received a Nodes Protocol Support Flags, set valid flag
    end
  end
end;

procedure HandleStreamMessage(CANBuffer: PCANBuffer; var LocalFlags: Word);
begin
  if NMRABusUtilities_IsAddressedMessageToNode(CANBuffer) then
  begin
  end
end;

// ****************************************************************************
// procedure HandlePacketReceivedOnNMRABusLayer
//
//  Description:
//                 All NMRABus Layer messages are low priority and placed in the List for
//     handling in the main message loop
//
// ****************************************************************************
procedure HandlePacketReceivedOnNMRABusLayer(CANBuffer: PCANBuffer);
var
  DoDefault: Boolean;
  LocalFlags: Word;
  i: Integer;
begin
  {$IFDEF FIX_NESTED_INTERRUPT_SILICON_BUG} asm DISI #4; end; {$ENDIF}
  C1IE_bit := 0;
  LocalFlags := 0;
  DoDefault := True;
  if NMRAnetLayerReceiveHook <> nil then 
    NMRAnetLayerReceiveHook(CANBuffer, DoDefault);                              // Allow the program to have a crack at the message handler
  if DoDefault then
  begin
    if NMRABusUtilities_EqualAliasID(CANBuffer^.ID and MASK_SOURCE_ALIAS) then 
      HandleEqualAliasID(CANBuffer, LocalFlags);                                // This is ALWAYS checked regardless of permitted state

    if NMRABus_Permitted then
    begin
      case CANBuffer^.ID and MTI_TYPE_MASK of
        MTI_UNADDRESSED              : HandleUnAddressedMessage(CANBuffer, LocalFlags);
        MTI_RESERVED                 : begin end;
        MTI_DATAGRAM_ONLY_FRAME,
        MTI_DATAGRAM_FRAME_START,
        MTI_DATAGRAM_FRAME,
        MTI_DATAGRAM_FRAME_END       : HandleDatagramMessage(CANBuffer, LocalFlags);
        MTI_ADDRESSED                : HandleAddressedMessage(CANBuffer, LocalFlags);
        MTI_STREAM_DATA_SEND         : HandleStreamMessage(CANBuffer, LocalFlags);
      end;
      NMRABus.Responses.SimpleResponseFlags := NMRABus.Responses.SimpleResponseFlags or LocalFlags;
    end;
  end;
  {$IFDEF FIX_NESTED_INTERRUPT_SILICON_BUG} asm DISI #4; end; {$ENDIF}
   C1IE_bit := 1;
end;

